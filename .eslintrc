{
  "extends": "airbnb",
  "rules": {
    "no-console": 0,
    "quote-props": 0,
    "no-restricted-syntax": 0,
    "guard-for-in": 0,
    "max-len": 0,
    "no-use-before-define": 0,
    "dot-notation": 0,
    "quotes": 0,
    "comma-dangle": 0,
    "class-methods-use-this": 0,
    "no-confusing-arrow": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "no-case-declarations": 0,
    "jsx-a11y/interactive-supports-focus": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "no-param-reassign": 0,
    "import/export": 0,
    "global-require": 0,
  },
  "globals": {
    "storage": false,
    "fetch": false,
    "btoa": false,
    "navigator": false,
    "window": false,
    "document": false,
    "localStorage": false,
    "location": false,
    "Parse": false,
  }
}
